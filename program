double area()
{
double a=4;
double r=4;
double sa=a*a;
double ra=pi*r*r;
double area=ra-sa;
println(area);
}

DATA VARIABLE
1.Using third variable/temp varible
int x=5;
int y=6;
int temp;
temp=x;
x=y;
y=temp;
println(x);
println(y);

2.without third variable
int x= 5;
int y = 6;
x=x+y;
x=11
y= x-y;
y = 5
x=x-y;
println(x);
Println(y);

3.Simple interest
double p  = 100000.00
double n  = 2.50 ;
double r  = 7.75;
double si = ( p*n*r*)/100.0;
Println (si);

4.method simple interest
double simint()
{
double p=100000.00;
double n=2.50;
double r=7.75;
double si=(p*n*r)/100;
println(si);
return si;
}

5.method average
double avg()
{
double average= ( 1.0,2.0)0.3)/3.0;
Print(average);
return average;
}
double sum ( double a, double b, double c)
{
double d=a+b+c ;
return d;
}

arithmetic.java
public class arithmetic
{ 
int a=5;
int b=7;
int add()
{
c=a+b;
println(c);
return c;
int sub()
{
c=a-b;
println(c);
return c;
}
main method
void main()
{
arithmetic call=new arithmetic();
call.add();
call.sub();
}
}

double a()
{
double b=7,h=5;
double area=0.5*b*h;
return area;
}
double b()
{
double b=5,h=7;
double area=0.5*b*h;
return area;
double a()
{
double d=a()>b()?a():b();
println(d);
return d;
}

How much is 40% of 420

percent.java
public class percent
{
int calculate()
{
int amount=420;
int percent=40%;
int ans=amount*(percent/100);
println(ans);
return ans;
}
void main()
{
percent obj=new percent();
obj.calculate();
}
}

syntax for method call
objct name.method name

36 is how much percentage in 78%?
public class percentage
{
double cal()
{
double amount1=36;
double amount2=78;
double own=(amount1/amount2*100);
return own;
}
void main()
{
percentage obj=new percentage();
obj.cal();
}

Example:
int height=;
boolean do you know swimming=;
if(do you know swimming==true)
{
if(height>=160) 
{
println("you are eligible");
}
else
{
println("you can go to kids pool");
}
}
else
println("go and learn swimming and come back");
}

EXAMBLE:
int age =14;
if(age<=2)
{
Println ("No ticket");
}
else
{
if(age<=11)
{
println("Half ticket");
}
else
{
println("Full ticket");
}
}


EXAMBLE FOR MULTIPLE CONDITION
boolean running= ;
boolean written= ;
boolean medical= ;
if(running==ture&&written== ture&& medical==ture)
{
println("you are selected");
}
else
double total Mark= ;
double cut of Mark= ;
if(total Mark>=960//cut of mark>=160)
{
println("you are eligible to apply for counciling");
}

SWITCH CASE (choice /selection based execution)
int day= 5;
Switch (day)
{
Case 1:
println ("Monday");
break;
Case 2:
println ("Tuesday");
break;
Case 3:
println ("Wednesday");
break;
Case 4:
println ("Thursday");
break;
Case 5:
println ("Friday");
break;
Case 6:
println ("Saturday");
break;
Case 7:
println ("Sunday");
break;
}

EXAMBLE:

int a=5;
int b=5;
int c;
Char symbol='*';
Switch (symbol)
{
Case '+':
C= a+b;
println (c);
break;
Case (-):
C=a-b;
break;
Case(*):
C=a*b;
break;
Case (/):
C= a/b;
break;
}

