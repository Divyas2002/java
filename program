break vs continue
----- --- -------
for(i=1;i<=10;i=i+1)
{
if(i==5)
{
break;
}
system.out.println(i);
}

for(inti=1;i<=100;i=i+1)
{
if(i==50)
{
break;}
System.out.println(i);
}

