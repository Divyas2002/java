SBI core Bank.java
Public Class SBI core Bank
{
Static int debitTXcounter=0;
Static int creditTXcounter=0;
       int accountNumber;
string customerName;
double accountBalance;
SBIcoreBank (int accountNumber, string customerName, double accountBalance)
{
this.accountNumber= accountNumber;
this.customerName= customerName;
this.accountBalance = accountBalance;
}
double credit(double amount)
{
System.out.println( accountNumber + ":" customerName);
System.out.println("Before credit"+ accountBalance);
accountBalance= accountBalance + amount;
System.out.println("After credit"+ accountBalance);
CreditTx2counter = creditTXcounter+1;
return accountBalance;
}
double debit(double amount)
{
System.out.println( accountNumber + ":" customerName);
System.out.println( "before debit " + accountBalance);
accountBalance= accountBalance - amount;
System.out.println( "after debit " + accountBalance);
debitTXcounter = debitTXcounter+1;
return accountBalance;
}
Public static void main (String arg[])
{
SBIcoreBank b1= new SBIcoreBank(123," Raj ",25000.0)
SBIcoreBank b2 = new SBIcoreBank(345,"Tej",15500.0)
SBIcoreBank b3 = new SBIcoreBank(567,"zara",47250.0)
 
 b1.debit (5000.0)
 b2.credit(500.0);
 b3.credit(1000.0);
 b1.credit(250.0);
 b2.debit (7000.0);
System.out.println(debitTXcounter);
System.out.println(creditTXcounter);
}
}


